{
  "name": "matchi",
  "version": "1.0.3",
  "description": "Matchi Game",
  "scripts": {
    "prestart": "npm run remove-dist",
    "start": "parallelshell \"npm run open:src\"",
    "open:src": "node tools/srcServer.js",
    "open:dist": "node tools/distServer.js",
    "lint": "eslint src",
    "lint:watch": "watch 'npm run lint' src",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "npm run test && node tools/build.js && npm run open:dist",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel/register --recursive \"./src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "startserver": "babel-node server.js --presets es2015,stage-2"
  },
  "author": "Norm Crandall",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "3.3.6",
    "bootstrap-webpack": "0.0.5",
    "ioredis": "1.14.0",
    "jquery": "2.2.0",
    "less": "2.5.3",
    "object-assign": "4.0.1",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "redux": "3.0.5",
    "socket.io": "1.4.4"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "4.1.6",
    "babel-loader": "5.1.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.0-beta1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "browser-sync": "2.11.1",
    "chai": "3.4.1",
    "cheerio": "0.19.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-loader": "1.2.0",
    "eslint-plugin-react": "3.15.0",
    "exports-loader": "0.6.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "less-loader": "2.2.2",
    "mocha": "2.3.4",
    "node-sass": "3.4.2",
    "parallelshell": "2.0.0",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "rimraf": "2.5.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "watch": "0.17.1",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0",
    "yargs": "3.32.0"
  }
}
